<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Maps.addFood">

    <Button
        android:id="@+id/mapsAdd_confirmBtn"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/Colour2"
        android:text="Confirm"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:height="100px"
        android:background="#DDFFBB"
        android:gravity="center"
        android:text="Donate Food"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="141dp"
        android:layout_height="26dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="126dp"
        android:text="Product"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="141dp"
        android:layout_height="27dp"
        android:layout_marginTop="51dp"
        android:text="Brand"
        app:layout_constraintEnd_toEndOf="@+id/textView19"
        app:layout_constraintStart_toStartOf="@+id/textView19"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="141dp"
        android:layout_height="26dp"
        android:layout_marginTop="56dp"
        android:text="Expiration Date"
        app:layout_constraintEnd_toEndOf="@+id/textView25"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView25"
        app:layout_constraintTop_toBottomOf="@+id/textView25" />

    <EditText
        android:id="@+id/mapAdd_ProducrName"
        android:layout_width="141dp"
        android:layout_height="40dp"
        android:layout_marginStart="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/textView19"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toTopOf="@+id/textView19" />

    <EditText
        android:id="@+id/mapsAdd_productBrand"
        android:layout_width="141dp"
        android:layout_height="40dp"
        android:layout_marginTop="51dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/mapAdd_ProducrName"
        app:layout_constraintStart_toStartOf="@+id/mapAdd_ProducrName"
        app:layout_constraintTop_toBottomOf="@+id/mapAdd_ProducrName" />

    <EditText
        android:id="@+id/mapsAdd_Expiration"
        android:layout_width="141dp"
        android:layout_height="40dp"
        android:layout_marginTop="51dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/mapsAdd_productBrand"
        app:layout_constraintStart_toStartOf="@+id/mapsAdd_productBrand"
        app:layout_constraintTop_toBottomOf="@+id/mapsAdd_productBrand" />
</androidx.constraintlayout.widget.ConstraintLayout>